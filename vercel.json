
{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/media/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!media|assets|favicon.ico).*)",
      "destination": "/index.html"
    }
  ]
}
